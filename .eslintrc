{
"parser": "babel-eslint",
"env": {
    "es6": true,
    "browser": true,
    "node": true
},
"rules": {
    "linebreak-style": "off",
    "semi": "off",
    "quotes": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "space-before-blocks": "off",
    "indent": "off"

},
"extends": ["eslint:recommended", "google"]
}